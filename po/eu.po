msgid "<b>Fingerprints</b>"
msgstr "<b>Hatz-markak</b>"

#: ../data/glade/certificate-dialogs.glade.h:2
msgid "<b>Issued By</b>"
msgstr "<b>Honek bidalita</b>"

#: ../data/glade/certificate-dialogs.glade.h:3
