#| msgid "Web Browser"
msgid "web-browser"
msgstr "web-browser"

#: data/epiphany.desktop.in:19
msgid "New Incognito Window"
msgstr "Yeni Gizli Tarama Penceresi"

#: data/org.gnome.epiphany.gschema.xml:10
msgid "Browse with caret"
msgstr "İmleç ile tara"

