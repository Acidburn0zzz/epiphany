      <title>Browsing the Web</title>
      <para>
      </para>
        <sect2 id="using-address-entry">
          <title>Using the Address Entry</title>
          <para>
          </para>
        </sect2>
	<sect2 id="using-toolbar">
          <title>Using the Toolbar</title>
