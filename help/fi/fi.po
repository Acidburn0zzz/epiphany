#. Tag: title
#: epiphany.xml:2132
#, no-c-format
msgid "Navigation Shortcuts"
msgstr "Selailun pikanäppäimet"

#. Tag: keycap
#: epiphany.xml:2154
#, no-c-format
msgid "Ctrl-N"
msgstr "Control-N"

#. Tag: para
#: epiphany.xml:2158
#, no-c-format
msgid "Open a new window."
msgstr "Avaa uusi ikkuna."

#. Tag: keycap
#: epiphany.xml:2166
#, no-c-format
